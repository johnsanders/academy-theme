{{> theme_boost/head }}
	<body {{{ bodyattributes }}} >
		{{> theme_academy/loader }}
		<script>
			window.cnnAcademy = { templateType: 'front_page_logged_in' };
		</script>
		{{> core/local/toast/wrapper}}
		<div
			id="page-wrapper"
			class="d-print-block"
		>
			{{{ output.standard_top_of_body_html }}}
			{{> theme_academy/navbar }}
			{{> theme_academy/nav_drawer }}
			<div id="academyContent"></div>
			<div
				id="page"
				class="container-fluid d-print-block loggedIn d-none"
			>
				<div id="page-content" class="d-none">
					{{{ output.main_content }}}
				</div>
			</div>
			<div id="academyFooter"></div>
		</div>
		{{{ output.standard_end_of_body_html }}}
	</body>
</html>
{{#js}}
	M.util.js_pending('theme_boost/loader');
	require(['theme_boost/loader'], function() {
		M.util.js_complete('theme_boost/loader');
	});
	M.util.js_pending('theme_boost/drawer');
	require(['theme_boost/drawer'], function(drawer) {
		cnnAcademy.drawer = drawer;
		setTimeout(() => {
			drawer.init();
			M.util.js_complete('theme_boost/drawer');
		}, 1000);
	});
{{/js}}