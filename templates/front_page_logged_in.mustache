{{> theme_academy/head }}
	<body {{{ bodyattributes }}} >
		{{> theme_academy/loader }}
		<script>
			cnnAcademy.templateType = 'front_page_logged_in';
		</script>
		{{> core/local/toast/wrapper }}
		<div
			id="page-wrapper"
			class="d-print-block"
		>
			{{{ output.standard_top_of_body_html }}}
			<div id="academyNavbar"></div>
			{{> theme_academy/nav_drawer }}
			<div id="academyContent"></div>
			<div
				id="page"
				class="container-fluid d-print-block loggedIn d-none"
			>
				<div id="page-content" class="d-none">
					{{{ output.main_content }}}
				</div>
			</div>
			<div id="academyFooter"></div>
		</div>
		{{{ output.standard_end_of_body_html }}}
	</body>
</html>
{{> theme_academy/shared_js }}